programa
{
	
	funcao inicio()
	{
	// 1 - Faça um programa que mostre o valor total da compra de todos os livros.
	
		real livro_hp1 = 26.90
		real livro_hp2 = 34.10
		real livro_hp3 = 29.90
		real livro_hp4 = 26.90
		real livro_hp5 = 50.00
		real livro_hp6 = 26.90
		real soma = livro_hp1+livro_hp2+livro_hp3+livro_hp4+livro_hp5+livro_hp6
		
		escreva("A soma de todos os valores dos livros é de: ", soma, " reais")
	
	}
}
